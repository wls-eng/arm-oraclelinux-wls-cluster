{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "value": "weblogic"
    },
    "adminPasswordOrKey": {
      "value": "#adminPasswordOrKey#"
    },
    "dnsLabelPrefix": {
      "value": "wls"
    },
    "acceptOTNLicenseAgreement": {
      "value": "Y"
    },
    "otnAccountUsername": {
      "value": "#otnAccountUsername#"
    },
    "otnAccountPassword": {
      "value": "#otnAccountPassword#"
    },
    "wlsDomainName": {
      "value": "#wlsdomainname#"
    },
    "wlsUserName": {
      "value": "#wlsusername#"
    },
    "wlsPassword": {
      "value": "#wlspassword#"
    },
    "managedServerPrefix":{
      "value": "#managedserverprefix#"
    },
    "numberOfInstances": {
      "value": #numinstances#
    },
    "adminVMName": {
      "value": "#adminvmname#"
    },
    "vmSizeSelect": {
      "value": "Standard_A3"
    },
    "location": {
      "value": "#location#"
    },
    "_artifactsLocation": {
      "value": "https://raw.githubusercontent.com/sanjaymantoor/arm-oraclelinux-wls-cluster/testcicd/arm-oraclelinux-wls-cluster/src/main/arm/"
    }
  }
}
